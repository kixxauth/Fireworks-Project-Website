<script id="item-popup-template" type="text/html">
	<h3 class="item-title">${name} <em class="item-classname">(${classname})</em></h3>
	<p class="item-description">${description}</p>
	<p class="item-score">Value: ${value}, Rank: ${rank}</p>
	<p class="item-content">{{html content}}</p>
	<h4>Dependencies:</h4>
	<ul>
	{{each(i, item) dependencies}}
	  <li>${item}%</li>
	{{/each}}
  </ul>
	<h4>Dependents:</h4>
	<ul>
	{{each(i, item) dependents}}
	  <li>${item}%</li>
	{{/each}}
  </ul>
</script>

